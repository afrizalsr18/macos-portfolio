{
    "compilerOptions": {
        "ignoreDeprecations": "6.0",
        "baseUrl": ".",
        "paths": {
            "#components": [
                "src/components/index.ts"
            ],
            "#components/*": [
                "src/components/*"
            ],
            "#constants": [
                "src/constants/index.ts"
            ],
            "#constants/*": [
                "src/constants/*"
            ],
            "#store/*": [
                "src/store/*"
            ],
            "#hoc/*": [
                "src/hoc/*"
            ],
            "#windows": [
                "src/windows/index.ts"
            ],
            "#windows/*": [
                "src/windows/*"
            ]
        }
    },
    "include": [
        "src"
    ]
}